    "id_naver = '(아이디)'\n",
    "pw_naver = '(비밀번호)'\n",
    "id_gmarket = '(아이디)'\n",
    "pw_gmarket = '(비밀번호)'\n",
    "id_11st = '(아이디)'\n",
    "pw_11st = '(비밀번호)'\n",
    "\n",
    "url_naver = '(상품페이지 주소)','(상품페이지 주소)'\n",
    "url_gmarket =  '(상품페이지 주소)','(상품페이지 주소)'\n",
    "url_11st =  '(상품페이지 주소)','(상품페이지 주소)'"
